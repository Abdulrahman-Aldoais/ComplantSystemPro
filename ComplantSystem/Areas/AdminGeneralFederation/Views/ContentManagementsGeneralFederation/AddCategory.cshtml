
@model TypeComplaint

<div class="content-wrapper">
    <!-- Content -->
    <!-- Offcanvas to add new user -->
    <div class="container-xxl flex-grow-1 container-p-y">
        <div class="offcanvas-header border-bottom">
            <h6 id="offcanvasAddUserLabel" class="offcanvas-title">اضافة صنف بلاغ</h6>
    
        </div>
        <div class="offcanvas-body flex-grow-0 mx-0" data-select2-id="6">
            <form asp-action="AddCategory" method="post">

              
                <div class="col-md-6 mb-3">
                    <label for="Type" class="form-label"> صنف الشكوى</label>
                    <input asp-for="Type" class="form-control"
                           type="text"
                           id="TatileProposal"
                          
                         placeholder="صنف الشكوى " />
                </div>
                <div class="col-md-6 mb-3 fv-plugins-icon-container buttom-top-margin">

                    <button type="submit" value="AddCategory" class="btn btn-primary d-grid w-100 mb-2">إضافة</button>
                    <button type="reset" class="btn btn-label-secondary">
                        إلغاء
                    </button>
                </div>
            </form>
               
        </div>
    </div>
    </div>
